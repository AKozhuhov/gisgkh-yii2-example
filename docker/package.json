{
  "name": "opengkh.evironment",
  "version": "1.0.0",
  "description": "GIS GKH integration environment example (for yii2-gisgkh module)",
  "scripts": {
    "docker": "./build.js && ./run.js",
    "docker-rebuild": "./build.js -f && ./run.js -f",
    "docker-update": "./build.js -f -u && ./run.js -f",
    "services": "./execute.js -u root -c /opengkh/docker/services/start.js",
    "services-restart": "./execute.js -u root -c /opengkh/docker/services/restart.js",
    "app-install": "./execute.js -u dvp -c /opengkh/install.sh"
  },
  "dependencies": {
    "app-root-path": "^1.2.1",
    "ip": "^1.1.3",
    "shell-done": "git://github.com/maxeko/shell-done.git",
    "shelljs": "^0.7.0",
    "userid": "^0.2.0",
    "username": "^2.2.2",
    "yargs": "^4.7.1"
  },
  "author": "Maxim Korshunov <korshunov.m.e@gmail.com>"
}
